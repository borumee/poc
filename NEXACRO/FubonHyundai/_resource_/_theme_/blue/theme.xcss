}
.Button.btn_WF_PageS,
.Button.btn_WF_PageS[status=focused],.Button.btn_WF_PageS[userstatus=selected],
.Button.btn_WF_PageS[status=mouseover],.Button.btn_WF_PageS[userstatus=pushed]
