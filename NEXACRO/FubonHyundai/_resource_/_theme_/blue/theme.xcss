}
/* Grid */
.Grid
{
	background : #ffffff;
	-nexa-border : 2px solid #004f9c,0px none,1px solid #b7b1b2;
	/* -nexa-border : 2px solid #004F9C, 1px solid #b7b1b2, 1px solid #b7b1b2, 1px solid #b7b1b2; */
}

/* head */
.Grid .head .row .cell
{
	font : bold 12px "Malgun Gothic";
	color : #2f2f2f;
	background :#F1F1F1;
	-nexa-border : 1px solid #DEDEDE,0px none;
	-nexa-padding : 4px 4px 4px 4px;
	-nexa-text-align : center;
}

.Grid .head .row .cell[status=disabled]
{
	color : #999999;
}

.Grid .body .row .cell[status=disabled][userstatus=selected]
{
	background : #f0f0f0;
}
