/* Frame/Form */
.Form
{
	font : 13px "Malgun Gothic";
	color : #111111;
	background : #ffffff;
	-nexa-border : 0px none;
}

.Form[userstatus=contents]
{
	background : transparent;
}

.Form[status=disabled]
{
	color : #999999;
}
.MainFrame,.ChildFrame,.FrameSet,.VFrameSet,.HFrameSet,.TileFrameSet
{
	/*-nexa-border : 1px solid #b7b1b2;*/
}

/* Button */
/* FileDownload */
/* FileUpload */
.Button,.FileDownload
{
	background : linear-gradient(to top,#eaeaea,#ffffff);
	-nexa-border : 1px solid #c2c2c2;
	cursor : pointer;
}

.ButtonControl
{
	background : transparent;
	-nexa-border : 0px none;
}

.Button.btn_WF_sampleMore,
.Button.btn_WF_sampleMore[status=focused]
{
	background : #D7DAE2;
	-nexa-border : 0px none;
	border-radius : 50%;
	-nexa-icon : url('imagerc::img_WF_sampleMore.png');
}
.Button.btn_WF_sampleMore[status=mouseover],
.Button.btn_WF_sampleMore[userstatus=pushed],
.Button.btn_WF_sampleMore[userstatus=selected]
{
	background : #C5C8D1;
	-nexa-border : 0px none;
}

.Button.btn_WF_sampleAdd,
.Button.btn_WF_sampleAdd[status=focused]
{
	-nexa-border : 0px none;
	background : #a598ef;
	-nexa-icon : url('imagerc::img_WF_sampleAdd.png');
	border-radius : 50%;
	box-shadow : 3px 3px 3px RGBA(84,84,84,0.20);
}
.Button.btn_WF_sampleAdd[status=mouseover],
.Button.btn_WF_sampleAdd[userstatus=pushed],
.Button.btn_WF_sampleAdd[userstatus=selected]
{
	-nexa-border : 0px none;
	background : #6954e1;
	-nexa-icon : url('imagerc::img_WF_sampleAdd.png');
}

.Button.btn_WF_samplesShare,
.Button.btn_WF_samplesShare[status=focused]
{
	-nexa-border : 1px solid #d7dae2;
	background : #ffffff;
	border-radius : 50%;
	-nexa-icon : url('imagerc::img_WF_samplesShare.png');
	
}
.Button.btn_WF_samplesShare[status=mouseover],
.Button.btn_WF_samplesShare[userstatus=pushed],
.Button.btn_WF_samplesShare[userstatus=selected]
{
	-nexa-border : 1px solid #D1CBF3;
	background : #E7E4FB;
}
.Button.btn_WF_sampleWrite,
.Button.btn_WF_sampleWrite[status=focused]
{
	-nexa-border : 1px solid #d7dae2;
	background : #ffffff;
	border-radius : 50%;
	-nexa-icon : url('imagerc::img_WF_sampleWrite.png');
	
}
.Button.btn_WF_sampleWrite[status=mouseover],
.Button.btn_WF_sampleWrite[userstatus=pushed],
.Button.btn_WF_sampleWrite[userstatus=selected]
{
	-nexa-border : 1px solid #D1CBF3;
	background : #E7E4FB;
}

.FileUpload
{
	/* No style applied */
}

.FileUpload .fileuploaditem
{
	/* No style applied */
}

.FileUpload .fileuploaditem[status=disabled]
{
	color : #999999;
}

.FileUpload .fileuploaditem .fileuploaditembutton
{
	background : linear-gradient(to top,#eaeaea,#ffffff);
	-nexa-border : 1px solid #c2c2c2;
	-nexa-padding : 1px 9px 0px 9px;
}

