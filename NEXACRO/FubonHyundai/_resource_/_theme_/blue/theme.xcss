}
.Button.btn_WF_PageR[status=disabled]
{
	-nexa-border 		: 0px none;
	background 		: URL('imagerc::Frame/btn_WF_PageRD.png') no-repeat center center #ffffff;
}
.Button.btn_WF_PageP,
.Button.btn_WF_PageP[status=focused]
{
	-nexa-border 		: 0px none;
	-nexa-padding 		: 0px 0px 0px 0px;
	background 		: URL('imagerc::Frame/btn_WF_Preview.png') no-repeat center center #ffffff;
}
.Button.btn_WF_PageP[status=mouseover],.Button.btn_WF_PageP[userstatus=pushed],.Button.btn_WF_PageP[userstatus=selected]
{
	-nexa-border 		: 0px none;
	background 		: URL('imagerc::Frame/btn_WF_Preview.png') no-repeat center center #ffffff;
}
.Button.btn_WF_PageP[status=disabled]
{
	-nexa-border 		: 0px none;
	background 		: URL('imagerc::Frame/btn_WF_PreviewD.png') no-repeat center center #ffffff;
}
.Button.btn_WF_PageN,
.Button.btn_WF_PageN[status=focused]
{
	-nexa-border 		: 0px none;
	-nexa-padding 		: 0px 0px 0px 0px;
	background 		: URL('imagerc::Frame/btn_WF_Next.png') no-repeat center center #ffffff;
}
.Button.btn_WF_PageN[status=mouseover],.Button.btn_WF_PageN[userstatus=pushed],.Button.btn_WF_PageN[userstatus=selected]
{
	-nexa-border 		: 0px none;
	background 		: URL('imagerc::Frame/btn_WF_Next.png') no-repeat center center #ffffff;
}
.Button.btn_WF_PageN[status=disabled]
{
	-nexa-border 		: 0px none;
	background 		: URL('imagerc::Frame/btn_WF_NextD.png') no-repeat center center #ffffff;
}
.Button.btn_WF_Page,
.Button.btn_WF_Page[status=focused],.Button.btn_WF_Page[userstatus=selected],
.Button.btn_WF_Page[status=mouseover],.Button.btn_WF_Page[userstatus=pushed]
