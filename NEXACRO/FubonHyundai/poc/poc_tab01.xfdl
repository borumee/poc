window.pieSeriesLabelFunc = function(seriesId, index, data, values)
{
	return data["cnsult_type"]+", "+data["cnt"]+"건";
};

window.dataTipFunc = function(seriesId, seriesName, index, xName, yName, data, values) {
   return "<b><font color='#CC3300'>itemIndex : " + index + "</b></font>"
			+"<br><b><font color='#CC3300'>data : </b></font>"
			+ "<b>" + obj2string(data) + "</b>"
			+"<b><br><font color='#CC3300'>대상 : </b></font><b>"+values[2] + "</b>"
			+"<b><br><font color='#CC3300'>건수 : </b></font><b>"+values[0] + "건" + "</b>"
			+"<b><br><font color='#CC3300'>비율 : </b></font><b>"+ (Math.round(values[1]*100) / 100) +"%</b>";
};

window.obj2string = function(values) {
   var rtnValue = "";
   var prop;
   
   for (prop in values) 
   {
      if (rtnValue.length > 0) rtnValue += "";
      
	  if (prop == "cnsult_type") rtnValue += "" + values[prop];
	  else rtnValue += ", " + values[prop] + "건";
   }
   
   return rtnValue;
};

