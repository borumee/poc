/************************************************************************************************
 * FORM 변수 선언 영역
 ************************************************************************************************/
this.poc01_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{	
	this.CHART_LAYOUT1 = this.layoutPieChart;
	this.CHART_LAYOUT2 = this.layoutPieChart;
	this.CHART_LAYOUT3 = this.layoutBarChart;
	this.CHART_LAYOUT4 = this.layoutBarChart;
	
	this.create_rChart1(this.divChart1);
	this.create_rChart2(this.divChart2);
	this.create_rChart3(this.divChart3);
	this.create_rChart4(this.divChart4);
};

/************************************************************************************************
 * FORM EVENT 영역(onload, onbeforeclose)
 ************************************************************************************************/
/************************************************************************************************
 * 공통버튼 Event 영역
 ************************************************************************************************/
/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역
 ************************************************************************************************/
this.fnComBtnOnClick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	switch (obj.name)
	{
		case "btnChartChange1" :
			if (obj.cssclass == "btn_WF_ChartBar") 
			{
				obj.set_cssclass("btn_WF_ChartPie");
				this.CHART_LAYOUT1 = this.layoutBarChart2;
				this.create_rChart1(this.divChart1);
			}
			else
			{
				obj.set_cssclass("btn_WF_ChartBar");
				this.CHART_LAYOUT1 = this.layoutPieChart;
				this.create_rChart1(this.divChart1);
			}
			break;
			
		case "btnChartChange2" :
			if (obj.cssclass == "btn_WF_ChartBar")
			{
				obj.set_cssclass("btn_WF_ChartPie");
				this.CHART_LAYOUT2 = this.layoutBarChart2;
				this.create_rChart2(this.divChart2);
			}
			else
			{
				obj.set_cssclass("btn_WF_ChartBar");
				this.CHART_LAYOUT2 = this.layoutPieChart;
				this.create_rChart2(this.divChart2);
			}		
			break;
			
		case "btnChartChange3" :
			if (obj.cssclass == "btn_WF_ChartArea")
			{
				obj.set_cssclass("btn_WF_ChartBar");
				this.CHART_LAYOUT3 = this.layoutLineChart;
				this.create_rChart3(this.divChart3);
			}
			else
			{
				obj.set_cssclass("btn_WF_ChartArea");
				this.CHART_LAYOUT3 = this.layoutBarChart;
				this.create_rChart3(this.divChart3);
			}		
			break;
			
		case "btnChartChange4" :
			if (obj.cssclass == "btn_WF_ChartArea")
			{
				obj.set_cssclass("btn_WF_ChartBar");
				this.CHART_LAYOUT4 = this.layoutLineChart;
				this.create_rChart4(this.divChart4);
			}
			else
			{
				obj.set_cssclass("btn_WF_ChartArea");
				this.CHART_LAYOUT4 = this.layoutBarChart;
				this.create_rChart4(this.divChart4);
			}			
			break;
	}	
};

